res <- lapply(1:2000, function(i) {
R <- sqrt(10)
x <- runif(1, -R, R)
y <- runif(1, -R, R)
if (x**2 + y**2 <= R**2) {
if (x**2 + (y-1)**2 <= 7) {
return(dplyr::tibble(x=x, y=y, col="out"))
}
if (((x >= -3 + sqrt(6)) &  (x <= 3 - sqrt(6))) | (y <= -sqrt(6))) {
return(dplyr::tibble(x=x, y=y, col="not significant"))
}
if (x < -3 + sqrt(6)) {
return(dplyr::tibble(x=x, y=y, col="Underexpressed"))
}
if (x > 3 - sqrt(6)) {
return(dplyr::tibble(x=x, y=y, col="Overrexpressed"))
}
return(dplyr::tibble(x=x, y=y, col="significant"))
} else {
return(dplyr::tibble(x=x, y=y, col="out"))
}
}) %>% do.call("bind_rows", .)
